{
    "run_name": "brainit_m5",
    "writer": true,

    "seed": 42,

    "train_split": 0.9,

    "batch_size": 256,
    "num_workers": 2,

    "model_type": "brainit",
    "optimizer": "adam",
    "scheduler": "CosineAnnealingLR",
    "scheduler_patience": 3,
    "scheduler_T_max": 50,
    "criterion": "CE",
    "lr": 0.001,
    "epochs": 50,
    
    "early_stopping": false,
    "patience": 10,
    "min_diff": 0.001,

    "use_flex": false,

    "use_pos_embed": true,
    "add_dropout": false,
    "mlp_end": false,
    "add_cls_token": true,
    "num_layers": 2,
    "transform_type": "foveation",
    "label_smoothing": 0.1,
    "ema": false,
    "trans_dropout_ratio": 0.0,
    "standard_scale": true,
    "resnet_layers": 3,
    "multiscale_tokenisation": false,
    "gamma_per_head": false,
    "use_token_mixer": false,
    "remove_zoom": false,

    "downsample_fraction": 0,
    "few_shot": false,
    "freeze_resnet_early": false,

    "attacker": false,
    "epsilon": false,
    "gaussian_std": false
}
