{
    "run_name": "zoom",
    "writer": true,

    "seed": 42,

    "train_split": 0.9,

    "batch_size": 256,
    "num_workers": 2,

    "model_type": "zoom",
    "optimizer": "adam",
    "scheduler": "ReduceLROnPlateau",
    "criterion": "CE",
    "lr": 0.001,
    "epochs": 50,
    
    "early_stopping": true,
    "patience": 10,
    "min_diff": 0.001,

    "use_flex": false,

    "use_pos_embed": false,
    "add_dropout": false,
    "mlp_end": true,
    "add_cls_token": false,
    "num_layers": 2
}
